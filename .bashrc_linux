#
# Keychain
#
if [ -x /usr/bin/keychain ]; then
	keychain $HOME/.ssh/id_rsa
	source $HOME/.keychain/$HOSTNAME-sh
fi

#
# rbenv
#
if [ -d $HOME/.rbenv ]; then
	addpath $HOME/.rbenv/bin
	eval "$(rbenv init -)"
fi

#
# golang
#
if [ -d /usr/local/go/bin ]; then
	export GOPATH=$HOME/go
	addpath $GOPATH/bin
fi
