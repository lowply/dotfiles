{
    "suggest.preferCompleteThanJumpPlaceholder":true,
    "languageserver": {
        "go": {
            "command": "gopls",
            "rootPatterns": ["go.mod",".git/"],
            "filetypes": ["go"],
            "initializationOptions": {
                "usePlaceholders": true
            }
        }
    },
    "go.disable": {
        "workspaceFolders": false
    }
}
