# date
date +%Y%m%d

# dstat
dstat -cdnlt --top-io --top-bio --top-cpu --top-cputime --top-cputime-avg --top-mem

# ps
ps auxwww --sort=rss

# open port on macOS
sudo lsof -PiTCP -sTCP:LISTEN | grep "IPv4"

# reset DNS cache on macOS
sudo killall -HUP mDNSResponder

# epoch now
date -ju '+%F %T %Z';

# epoch
date -jur $1 '+%F %T %Z';

# kill AMPLibraryAgent
killall AMPLibraryAgent

# cd dotfiles on CS
cd ${DOTFILES_DIR}

# source sshauthsock.sh
source ${DOTFILES_DIR}/bin/sshauthsock.sh

# cd to iCloud Drive
cd ~/Library/Mobile\ Documents/com~apple~CloudDocs
