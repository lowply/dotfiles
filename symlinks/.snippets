# date
date +%Y%m%d

# dstat 
dstat -cdnlt --top-io --top-bio --top-cpu --top-cputime --top-cputime-avg --top-mem

# ps
ps auxwww --sort=rss

# open port on macOS
sudo lsof -PiTCP -sTCP:LISTEN | grep "IPv4"

# reset DNS cache on macOS
sudo killall -HUP mDNSResponder

# epoch now
date -ju '+%F %T %Z';

# epoch
date -jur $1 '+%F %T %Z';

# kill AMPLibraryAgent
killall AMPLibraryAgent

# cd dotfiles on CS
cd /workspaces/.codespaces/.persistedshare/dotfiles/

# SSH_AUTH_SOCK in CS
for x in $(find /tmp -type s -path "/tmp/ssh-*" 2>/dev/null); do if SSH_AUTH_SOCK=${x} ssh-add -l > /dev/null; then export SSH_AUTH_SOCK=${x}; echo "Done"; break; fi; done

# cd to iCloud Drive
cd ~/Library/Mobile\ Documents/com~apple~CloudDocs
